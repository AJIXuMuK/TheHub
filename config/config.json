{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "the-hub-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/theHub/TheHubWebPart.js",
          "manifest": "./src/webparts/theHub/TheHubWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "vue": "./node_modules/vue/dist/vue.min.js",
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      "globalName": "jquery"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js",
      "globalName": "bootstrap",
      "globalDependencies": ["jquery"]
    },
    "primedropdown":{
      "path": "https://unpkg.com/primevue/components/dropdown/dropdown.umd.min.js",
      "globalName": "primedropdown"
    },
    "primebutton":{
      "path": "https://unpkg.com/primevue/components/button/button.umd.min.js",
      "globalName": "primebutton"
    },
    "primedatatable":{
      "path": "https://unpkg.com/primevue/components/datatable/datatable.umd.min.js",
      "globalName": "primedatatable"
    },
    "yammer":{
      "path": "https://c64.assets-yammer.com/assets/platform_embed.js",
      "globalName": "yammer"
    }
  },
  "localizedResources": {
    "TheHubWebPartStrings": "lib/webparts/theHub/loc/{locale}.js"
  }
}